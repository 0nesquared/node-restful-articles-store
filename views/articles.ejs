<html>
    <head>
        <title>Article-Store</title>
    </head>
    <body>
        <div class="container">
            <h1>Article-Store</h1>
            <ul>
                <% articles.forEach((article) => { %>
                <li>
                    <a href="/articles/<%= article.id %>"> <%= article.title %> </a>
                </li>
                <% }) %>
            </ul>
        </div>
    </body>
</html>
